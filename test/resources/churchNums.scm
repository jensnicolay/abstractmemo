(let
    ((plus (lambda (n1 n2)
             (lambda (f) (lambda (x) ((n1 f) ((n2 f) x)))))))
  (let ((mult (lambda (n1 n2)
                (lambda (f) (n2 (n1 f))))))
    (let ((pred (lambda (n)
                  (lambda (f)
                    (lambda (x)
                      (((n (lambda (g) (lambda (h) (h (g f)))))
                        (lambda (ignored) x))
                       (lambda (id) id)))))))
      (let ((sub (lambda (n1 n2)
                   ((n2 pred) n1))))
        (let ((church0 (lambda (f) (lambda (x) x))))
          (let ((church1 (lambda (f) (lambda (x) (f x)))))
            (let ((church2 (lambda (f) (lambda (x) (f (f x))))))
              (let ((church3 (lambda (f) (lambda (x) (f (f (f x)))))))
                (let ((church0? (lambda (n)
                                  ((n (lambda (x) #f)) #t))))
                  (letrec ((church=? (lambda (n1 n2)
                                       (if (church0? n1) 
                                           (church0? n2)
                                           (if (church0? n2)
                                               #f
                                               (church=? (sub n1 church1) (sub n2 church1)))))))
                    ;; multiplication distributes over addition
                    (church=? (mult church2 (plus church1 church3))
                              (plus (mult church2 church1) (mult church2 church3)))))))))))))